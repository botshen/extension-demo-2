{
  "name": "extension-demo-2",
  "displayName": "Extension demo 2",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "ajax-hook": "^2.1.3",
    "plasmo": "0.67.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@types/chrome": "0.0.228",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "prettier": "2.8.7",
    "typescript": "5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "interceptor.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "permissions": [
      "storage",
      "tabs",
      "webRequest",
      "activeTab",
      "unlimitedStorage"
    ]
  }
}
